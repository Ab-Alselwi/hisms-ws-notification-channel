{
    "name": "ab-alselwi/hisms-ws-notification-channel",
    "description": "Hisms.ws Notification Channel for laravel.",
    "keywords": ["laravel", "notifications", "mobaily sms","Hisms"],
    "homepage": "https://github.com/alhoqbani/laravel-mobily-ws-notification",
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulfattah Alselwi",
            "email": "ab@droub.net"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^7.0.1",
        "illuminate/notifications": " ^8.0",
        "illuminate/support": " ^8.0",
        "illuminate/events": " ^8.0",
        "illuminate/queue": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\HismsWs\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\HismsWs\\Test\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "laravel": {
            "providers": [
                 "NotificationChannels\\HismsWs\\HismsWsChannelServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
